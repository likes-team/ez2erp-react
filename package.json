{
  "name": "isomorphic",
  "version": "7.5.0",
  "description": "A nextjs template to build dashboard",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "lint": "turbo lint",
    "type:check": "turbo type:check",
    "build": "turbo build",
    "start": "turbo start",
    "clean": "turbo clean && rm -rf node_modules .turbo",
    "cache:clean": "turbo cache:clean && rm -rf .turbo",
    "ez2erp:dev": "turbo dev --filter=ez2erp",
    "ez2erp:lint": "turbo lint --filter=ez2erp",
    "ez2erp:build": "turbo build --filter=ez2erp",
    "ez2erp:start": "turbo start --filter=ez2erp",
    "ez2erp:clean": "turbo clean --filter=ez2erp",
    "gen-icons": "turbo gen-icons --filter=core",
    "prepare": "husky"
  },
  "devDependencies": {
    "eslint": "9.17.0",
    "husky": "^9.1.7",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "turbo": "2.3.3",
    "typescript-config": "workspace:*"
  },
  "packageManager": "pnpm@9.1.4"
}
